{
    "manifest_version": 2,
    "name": "Skip Ad",
    "version": "0.0.0",
    "description": "Skip ad in video tag on pages by keyboard shortcut",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "64": "icon64.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "all_frames": true,
            "js": [
                "src/js/skipad.js"
            ]
        }
    ]
}  